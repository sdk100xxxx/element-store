{
  "name": "element-store",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "prisma generate && next dev",
    "dev:share": "prisma generate && next dev --hostname 0.0.0.0",
    "build": "prisma generate && next build",
    "start": "next start",
    "lint": "next lint",
    "postinstall": "prisma generate",
    "db:push": "prisma db push",
    "prisma:fix": "node -e \"const fs=require('fs');const p='node_modules/.prisma/client';if(fs.existsSync(p)){fs.rmSync(p,{recursive:true});console.log('Removed .prisma/client');}\" && prisma generate && prisma db push",
    "db:studio": "prisma studio",
    "db:seed": "npx ts-node -P tsconfig.seed.json prisma/seed.ts"
  },
  "dependencies": {
    "next": "14.2.15",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "@prisma/client": "^5.22.0",
    "next-auth": "^4.24.10",
    "stripe": "^17.4.0",
    "zod": "^3.23.8",
    "bcryptjs": "^2.4.3",
    "nanoid": "^5.0.9",
    "recharts": "^2.15.0",
    "dotenv": "^16.4.5",
    "@vercel/blob": "^0.25.0",
    "resend": "^4.0.0"
},
  "prisma": {
    "seed": "npx ts-node -P tsconfig.seed.json prisma/seed.ts"
  },
  "devDependencies": {
    "typescript": "^5.6.3",
    "@types/node": "^22.9.0",
    "@types/react": "^18.3.12",
    "@types/react-dom": "^18.3.1",
    "@types/bcryptjs": "^2.4.6",
    "autoprefixer": "^10.4.20",
    "postcss": "^8.4.49",
    "tailwindcss": "^3.4.15",
    "eslint": "^8.57.1",
    "eslint-config-next": "14.2.15",
    "prisma": "^5.22.0"
  }
}
